{
  "build": {
    "beforeDevCommand": "yarn dev",
    "devPath": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "distDir": "../dist"
  },
  "package": {
    "productName": "test tauri dev tools",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "windows": [
      {
        "title": "test tauri dev tools",
        "width": 1400,
        "height": 900
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.test-tauri-dev-tools",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
